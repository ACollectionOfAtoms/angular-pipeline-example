{
  "name": "angular-pipeline-example",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "i18n-extract": "ng xi18n --output-path=locale",
    "build": "ng build",
    "build-prod": "npm run build-stamp && npm run build-prod-en && npm run build-prod-es",
    "build-prod-en": "ng build -c production-en",
    "build-prod-es": "ng build -c production-es",
    "lint": "ng lint",
    "test": "ng test",
    "test-ci": "ng test -c ci",
    "audit-ci": "[ $(npm audit --json | jq '.metadata.vulnerabilities.moderate + .metadata.vulnerabilities.high + .metadata.vulnerabilities.critical') = \"0\" ] || npm audit",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e -c ci",
    "update-webdriver-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.46",
    "build-stamp": "./bin/build-stamp src/environments/build-info.ts",
    "postinstall": "npm run build-stamp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.13",
    "@angular/common": "~7.2.13",
    "@angular/compiler": "~7.2.13",
    "@angular/core": "~7.2.13",
    "@angular/forms": "~7.2.13",
    "@angular/platform-browser": "~7.2.13",
    "@angular/platform-browser-dynamic": "~7.2.13",
    "@angular/router": "~7.2.13",
    "core-js": "~2.6.5",
    "rxjs": "~6.3.3",
    "tslib": "~1.9.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.13",
    "@angular/language-service": "~7.2.13",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "ajv": "~6.10.0",
    "codelyzer": "~4.5.0",
    "jasmine-allure-reporter": "~1.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "protractor": "~5.4.0",
    "ts-morph": "~1.3.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
